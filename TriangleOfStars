using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
class TriangleOfStars
{
    static void Main(string[] args)
    {
        for (int row = 1; row < 11; row++)
        {
            for (int col = 0; col < row; col++)
            {
                Console.Write("*",row);
            }
            Console.WriteLine();

        }

    }
}
